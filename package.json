{
    "name": "oidc-spa",
    "version": "3.0.1",
    "description": "Openidconnect client for Single Page Applications",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/oidc-spa.git"
    },
    "scripts": {
        "build": "tsc",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different",
        "link-in-app": "ts-node --skipProject scripts/link-in-app.ts",
        "start-react-router-example": "yarn build && ts-node --skipProject scripts/link-in-examples.ts && cd examples/react-router && rm -rf node_modules/.vite && yarn dev",
        "start-tanstack-router-example": "yarn build && ts-node --skipProject scripts/link-in-examples.ts && cd examples/tanstack-router && rm -rf node_modules/.vite && yarn dev"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*.js"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "dist/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/garronej/oidc-spa",
    "devDependencies": {
        "@types/node": "^20.2.1",
        "@types/react": "^18.2.6",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^2.8.8",
        "react": "^18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4",
        "zod": "^3.22.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "jwt-decode": "^3.1.2",
        "oidc-client-ts": "^2.3.0",
        "tsafe": "^1.6.5"
    }
}
